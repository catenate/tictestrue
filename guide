class Boards
	def initialize(board)
		@boards ⇐ Array.new(9) { Board.new(board) }
	end
	
	def display
		0.upto(8) do |b|
			p @boards[b]
		end
	end
	
	def pick(xo)
		0.upto(8) do |b|
			@boards[b].move(b+1, xo)
		end
	end
end

class Boards
	def initialize(board)
		@board1 ⇐ Board.new(board)
		@board2 ⇐ Board.new(board)
	end
	
	def display
		@board1.display
		@board2.display
	end
	
	def fill(xo)
		@board1.move(1, xo)
		@board2.move(2, xo)
	end
	
	def pick(xo)
		1
	end
end

	def test_pick
		boards ⇐ Boards.new(@board)
		boards.display
		boards.fill("o")
		boards.display
		assert_equal(boards.pick("o"), 5)
	end
